{
  "name": "podcast-aggregator",
  "version": "0.1.0",
  "description": "A project of Chingu-Voyage 5",
  "main": "server/src/loader.js",
  "scripts": {
    "dev": "nodemon",
    "production": "pm2 start server/src/loader.js --name podcast-backend",
    "test": "./node_modules/.bin/mocha --timeout 5000 server/test/**/*.spec.js --require babel-register",
    "test:tdd": "./node_modules/.bin/mocha --timeout 5000 server/test/**/*.spec.js --require babel-register -w",
    "test:routes": "./node_modules/.bin/mocha --timeout 5000 server/test/routes.spec.js --require babel-register -w",
    "test:coverage": "nyc npm test",
    "coveralls": "npm run test:coverage && nyc report --reporter=text-lcov | coveralls",
    "build": "webpack",
    "build-babel": "babel client/src/app.js --out-file client/public/scripts/app.js --presets=env,react"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/chingu-voyage5/Geckos-Team-11.git"
  },
  "keywords": [
    "podcast",
    "chingu"
  ],
  "author": "Gleider, Miguel, Rachel",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/chingu-voyage5/Geckos-Team-11/issues"
  },
  "homepage": "https://github.com/chingu-voyage5/Geckos-Team-11#readme",
  "dependencies": {
    "bcrypt": "^2.0.1",
    "body-parser": "^1.18.3",
    "bootstrap": "^4.1.1",
    "express": "^4.16.3",
    "express-query-int": "^3.0.0",
    "jquery": "^3.3.1",
    "jsonwebtoken": "^8.3.0",
    "lodash": "^4.17.10",
    "mongoose": "^5.1.5",
    "mongoose-paginate": "^5.0.3",
    "mongoose-unique-validator": "^2.0.1",
    "node-restful": "^0.2.6",
    "pm2": "^2.10.4",
    "popper.js": "^1.14.3"
  },
  "devDependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.0-14",
    "@fortawesome/free-solid-svg-icons": "^5.1.0-11",
    "@fortawesome/react-fontawesome": "0.1.0-11",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.4",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "babel-register": "^6.26.0",
    "chai": "^4.1.2",
    "chai-http": "^4.0.0",
    "coveralls": "^3.0.1",
    "css-loader": "^0.28.11",
    "mocha": "^5.2.0",
    "node-sass": "^4.9.0",
    "nodemon": "^1.17.5",
    "normalize.css": "^8.0.0",
    "nyc": "^12.0.2",
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "react-modal": "^3.4.5",
    "rimraf": "^2.6.2",
    "sass-loader": "^7.0.3",
    "sinon": "^6.0.0",
    "sinon-chai": "^3.2.0",
    "sinon-stub-promise": "^4.0.0",
    "style-loader": "^0.21.0",
    "webpack": "^4.15.1",
    "webpack-cli": "^3.0.4"
  }
}
